<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Your login has failed</title>
    <link rel="stylesheet" type="text/css" href="/stylesheets/invariant.css">
    <link rel="stylesheet" href="./stylesheets/loginPages.css">
  </head>
  <body>
    <div class="loginNotice">
      <% rows.forEach(function(r) { %>
        <h2>You have successfully logged into the account <%= r.username %>. </h2>
        <form class="" action="/userView" method="get">
          <input class="submitButtons" type="submit" name="userViewRedirect" value="Go to forum">
        </form>
      <% }); %>
    </div>
  </body>
</html>
